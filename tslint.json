{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "max-classes-per-file": [
      false
    ],
    "member-ordering": false,
    "no-console": false,
    "object-literal-shorthand": false,
    "ordered-imports": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
